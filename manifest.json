{

  "description": "Adds some links for attachment on Jira",
  "manifest_version": 2,
  "name": "JiraAttachmentLink",
  "version": "1.0",
  "homepage_url": "https://github.com/CrazyB0y/JiraAttachmentLinks",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.atlassian.net/browse/*"],
      "js": ["jira-at-links.js"]
    }
  ]

}
